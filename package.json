{
  "name": "etcswap-sdks",
  "version": "0.0.0",
  "private": true,
  "description": "ETCswap SDK monorepo for Ethereum Classic DEX integration",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etcswap/sdks.git"
  },
  "keywords": [
    "etcswap",
    "ethereum-classic",
    "etc",
    "dex",
    "sdk",
    "uniswap"
  ],
  "author": "ETCswap",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/etcswap/sdks/issues"
  },
  "homepage": "https://github.com/etcswap/sdks#readme",
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.7.1",
  "scripts": {
    "build": "pnpm -r build",
    "clean": "pnpm -r clean",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "format": "prettier --write \"sdks/**/*.ts\"",
    "format:check": "prettier --check \"sdks/**/*.ts\""
  },
  "devDependencies": {
    "prettier": "^3.2.0",
    "typescript": "^5.3.0"
  }
}
